{
  "name": "publish-svelte",
  "version": "0.6.0",
  "description": "A command-line tool for publishing svelte components to npm",
  "prepublishOnly": "tsc",
  "main": "dist/pelte.js",
  "scripts": {
    "test": "mocha -r ts-node/register test/**.ts",
    "tsc": "tsc"
  },
  "bin": {
    "pelte": "dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "svelte",
    "cli",
    "publish",
    "share",
    "npm",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/philter87/publish-svelte"
  },
  "author": "Philip Christiansen",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "commander": "^4.0.1",
    "postcss": "^8.1.4",
    "prompts": "^2.3.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "svelte": "^3.12.1",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "lodash": "^4.17.15",
    "mocha": "^6.2.2",
    "ts-node": "^8.4.1",
    "tslint": "^6.0.0-beta0"
  }
